@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix mod:   <http://portalmod.com/ns/extensions/effect#> .

<http://portalmod.com/plugins/tap/pitch>
a lv2:Plugin, lv2:SpectralPlugin, lv2:PitchPlugin;

doap:name "TAP Pitch Shifter";

mod:label "TAP Pitch Shift";

doap:developer [
    foaf:name "Tom Szilagyi";
    foaf:homepage <http://tap-plugins.sourceforge.net/index.html>;
    foaf:mbox <mailto:tszilagyi@users.sourceforge.net>;
    ];

doap:maintainer [
    foaf:name "MOD Team";
    foaf:homepage <http://portalmod.com>;
    foaf:mbox <mailto:devel@portalmod.com>;
    ];

doap:license <http://usefulinc.com/doap/licenses/gpl>;
lv2:optionalFeature lv2:hardRTCapable;
lv2:minorVersion 0;
lv2:microVersion 1;
lv2:port
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 0;
    lv2:symbol "Semitone";
    lv2:name "Semitone Shift";
    lv2:default 0;
    lv2:minimum -12;
    lv2:maximum 12;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 1;
    lv2:symbol "Rate";
    lv2:name "Rate Shift [%]";
    lv2:default 0;
    lv2:minimum -50;
    lv2:maximum 100;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 2;
    lv2:symbol "Drylevel";
    lv2:name "Dry Level [dB]";
    lv2:default -90;
    lv2:minimum -90;
    lv2:maximum 20;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 3;
    lv2:symbol "Wetlevel";
    lv2:name "Wet Level [dB]";
    lv2:default 0;
    lv2:minimum -90;
    lv2:maximum 20;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 4;
    lv2:symbol "Latency";
    lv2:name "latency";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 16027;
],
[
    a lv2:InputPort, lv2:AudioPort;
    lv2:index 5;
    lv2:symbol "Input";
    lv2:name "Input";
],
[
    a lv2:OutputPort,  lv2:AudioPort;
    lv2:index 6;
    lv2:symbol "Output";
    lv2:name "Output";
].

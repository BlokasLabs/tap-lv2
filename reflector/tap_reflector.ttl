@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix mod: <http://portalmod.com/ns/modgui#>.
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix time: <http://lv2plug.in/ns/ext/time/#>.

<http://portalmod.com/plugins/tap/reflector>
a lv2:Plugin, lv2:DelayPlugin;

doap:name "TAP Reflector";

doap:developer [
    foaf:name "Tom Szilagyi";
    foaf:homepage <http://tap-plugins.sourceforge.net/index.html>;
    foaf:mbox <mailto:tszilagyi@users.sourceforge.net>;
    ];

doap:maintainer [
    foaf:name "MOD Team";
    foaf:homepage <http://portalmod.com>;
    foaf:mbox <mailto:devel@portalmod.com>;
    ];

doap:license <http://usefulinc.com/doap/licenses/gpl>;
lv2:optionalFeature lv2:hardRTCapable;



lv2:minorVersion 7;
lv2:microVersion 2;
lv2:port
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 0;
    lv2:symbol "fragment";
    lv2:name "Fragment Length";
    lv2:default 1000;
    lv2:minimum 100;
    lv2:maximum 1600;
    lv2:designation  time:beatsPerMinute;
    units:unit units:ms ;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 1;
    lv2:symbol "dry";
    lv2:name "Dry Level";
    lv2:default 0;
    lv2:minimum -90;
    lv2:maximum 20;
    units:unit units:db ;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 2;
    lv2:symbol "wet";
    lv2:name "Wet Level";
    lv2:default 0;
    lv2:minimum -90;
    lv2:maximum 20;
    units:unit units:db ;
],
[
    a lv2:InputPort, lv2:AudioPort;
    lv2:index 3;
    lv2:symbol "input";
    lv2:name "Input";
],
[
    a lv2:OutputPort,  lv2:AudioPort;
    lv2:index 4;
    lv2:symbol "output";
    lv2:name "Output";
];
mod:gui [
        a mod:Gui;
        mod:resourcesDirectory <modgui>;
        mod:iconTemplate <modgui/icon-tap-reflector.html>;
        mod:templateData <modgui/data-tap-reflector.json>;
        mod:screenshot <modgui/screenshot-tap-reflector.png>;
        mod:thumbnail <modgui/thumb-tap-reflector.png>;
    ] .

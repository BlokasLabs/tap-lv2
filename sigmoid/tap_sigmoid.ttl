@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix mod:   <http://portalmod.com/ns/extensions/effect#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://portalmod.com/plugins/tap/sigmoid>
a lv2:Plugin, lv2:DistortionPlugin;

doap:name "TAP Sigmoid Booster";

mod:label "TAP Sigmoid";

doap:developer [
    foaf:name "Tom Szilagyi";
    foaf:homepage <http://tap-plugins.sourceforge.net/index.html>;
    foaf:mbox <mailto:tszilagyi@users.sourceforge.net>;
    ];

doap:maintainer [
    foaf:name "MOD Team";
    foaf:homepage <http://portalmod.com>;
    foaf:mbox <mailto:devel@portalmod.com>;
    ];

doap:license <http://usefulinc.com/doap/licenses/gpl>;
lv2:optionalFeature lv2:hardRTCapable;
lv2:minorVersion 7;
lv2:microVersion 2;

rdfs:comment """
This plugin applies a time-invariant nonlinear amplitude transfer function to the signal. Depending on the signal and the plugin settings, various related effects (compression, soft limiting, emulation of tape saturation, mild distortion) can be achieved.

source: http://tap-plugins.sourceforge.net/ladspa/sigmoid.html
""";

lv2:port
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 0;
    lv2:symbol "Pregain";
    lv2:name "Pregain";
    lv2:default 0;
    lv2:minimum -90;
    lv2:maximum 20;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 1;
    lv2:symbol "Postgain";
    lv2:name "Postgain";
    lv2:default 0;
    lv2:minimum -90;
    lv2:maximum 20;
],
[
    a lv2:InputPort, lv2:AudioPort;
    lv2:index 2;
    lv2:symbol "Input";
    lv2:name "Input";
],
[
    a lv2:OutputPort, lv2:AudioPort;
    lv2:index 3;
    lv2:symbol "Output";
    lv2:name "Output";
].

@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extension/units#> .
@prefix mod: <http://portalmod.com/ns/modgui#>.	

<http://portalmod.com/plugins/tap/autopan>
a lv2:Plugin, lv2:SpatialPlugin;

doap:name "TAP AutoPanner";

doap:developer [
    foaf:name "Tom Szilagyi";
    foaf:homepage <http://tap-plugins.sourceforge.net/>;
    foaf:mbox <mailto:tsziagyi@users.sourceforge.net>;
    ];

doap:maintainer [
    foaf:name "MOD Team";
    foaf:homepage <http://portalmod.com>;
    foaf:mbox <devel@portalmod.com>;
    ];

doap:license <http://usefulinc.com/doap/licenses/gpl>;
lv2:optionalFeature lv2:hardRTCapable;


lv2:minorVersion 7;
lv2:microVersion 2;
lv2:port
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 0;
    lv2:symbol "Frequency";
    lv2:name "Frequency";
    lv2:default 3;
    lv2:minimum 0;
    lv2:maximum 20;
    units:unit units:hz ;

],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 1;
    lv2:symbol "Depth";
    lv2:name "Depth";
    lv2:default 80;
    lv2:minimum 0;
    lv2:maximum 100;
    units:unit units:pc ;

],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 2;
    lv2:symbol "Gain";
    lv2:name "Gain";
    lv2:default 0;
    lv2:minimum -70;
    lv2:maximum 20;
    units:unit units:db ;

],
[
    a lv2:InputPort, lv2:AudioPort;
    lv2:index 3;
    lv2:symbol "InputL";
    lv2:name "Input L";
],
[
    a lv2:InputPort, lv2:AudioPort;
    lv2:index 4;
    lv2:symbol "InputR";
    lv2:name "Input R";
],
[
    a lv2:OutputPort, lv2:AudioPort;
    lv2:index 5;
    lv2:symbol "OutputL";
    lv2:name "Output L";
],
[
    a lv2:OutputPort, lv2:AudioPort;
    lv2:index 6;
    lv2:symbol "OutputR";
    lv2:name "Output R";
];
mod:gui [
        a mod:Gui;
        mod:resourcesDirectory <modgui>;
        mod:iconTemplate <modgui/icon-tap-autopanner.html>;
        mod:templateData <modgui/data-tap-autopanner.json>;
        mod:screenshot <modgui/screenshot-tap-autopanner.png>;
        mod:thumbnail <modgui/thumb-tap-autopanner.png>;
    ] .

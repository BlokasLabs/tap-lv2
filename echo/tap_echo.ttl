@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix mod:   <http://portalmod.com/ns/extensions/effect#> .

<http://portalmod.com/plugins/tap/echo>
a lv2:Plugin, lv2:DelayPlugin;

doap:name "TAP Stereo Echo";

mod:label "TAP Stereo Echo";

doap:developer [
    foaf:name "Tom Szilagyi";
    foaf:homepage <http://tap-plugins.sourceforge.net/>;
    foaf:mbox <mailto:tsziagyi@users.sourceforge.net>;
    ];

doap:maintainer [
    foaf:name "MOD Team";
    foaf:homepage <http://portalmod.com>;
    foaf:mbox <mailto:devel@portalmod.com>;
    ];

doap:license <http://usefulinc.com/doap/licenses/gpl>;
lv2:optionalFeature lv2:hardRTCapable;
lv2:minorVersion 0;
lv2:microVersion 2;
lv2:port
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 0;
    lv2:symbol "LDelayMs";
    lv2:name "L Delay [ms]";
    lv2:default 300;
    lv2:minimum 0;
    lv2:maximum 2000;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 1;
    lv2:symbol "LFeedback";
    lv2:name "L Feedback [%]";
    lv2:default 50;
    lv2:minimum 0;
    lv2:maximum 100;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 2;
    lv2:symbol "RHaasDelayMs";
    lv2:name "R/Haas Delay [ms]";
    lv2:default 300;
    lv2:minimum 0;
    lv2:maximum 2000;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 3;
    lv2:symbol "RHaasFeedback";
    lv2:name "R/Haas Feedback [%]";
    lv2:default 50;
    lv2:minimum 0;
    lv2:maximum 100;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 4;
    lv2:symbol "LEchoLevalDb";
    lv2:name "L Echo Leval [dB]";
    lv2:default -4;
    lv2:minimum -70;
    lv2:maximum 10;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 5;
    lv2:symbol "REchoLevalDb";
    lv2:name "R Echo Leval [dB]";
    lv2:default -4;
    lv2:minimum -70;
    lv2:maximum 10;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 6;
    lv2:symbol "DryLevelDb";
    lv2:name "Dry Level [dB]";
    lv2:default -4;
    lv2:minimum -70;
    lv2:maximum 10;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 7;
    lv2:symbol "CrossMode";
    lv2:name "Cross Mode";
    lv2:default 1;
    lv2:minimum 0;
    lv2:maximum 1;
    lv2:portProperty lv2:toggled;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 8;
    lv2:symbol "HaasEffect";
    lv2:name "Haas Effect";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 1;
    lv2:portProperty lv2:toggled;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 9;
    lv2:symbol "SwapOutputs";
    lv2:name "Swap Outputs";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 1;
    lv2:portProperty lv2:toggled;
],
[
    a lv2:InputPort, lv2:AudioPort;
    lv2:index 10;
    lv2:symbol "InputLeft";
    lv2:name "Input Left";
],
[
    a lv2:OutputPort, lv2:AudioPort;
    lv2:index 11;
    lv2:symbol "OutputLeft";
    lv2:name "Output Left";
],
[
    a lv2:InputPort, lv2:AudioPort;
    lv2:index 12;
    lv2:symbol "InputRight";
    lv2:name "Input Right";
],
[
    a lv2:OutputPort, lv2:AudioPort;
    lv2:index 13;
    lv2:symbol "OutputRight";
    lv2:name "Output Right";
].

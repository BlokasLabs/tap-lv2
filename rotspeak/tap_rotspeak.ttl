@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix mod: <http://portalmod.com/ns/modgui#>.

<http://portalmod.com/plugins/tap/rotspeak>
a lv2:Plugin, lv2:ModulatorPlugin;

doap:name "TAP Rotary Speaker";


doap:developer [
    foaf:name "Tom Szilagyi";
    foaf:homepage <http://tap-plugins.sourceforge.net/index.html>;
    foaf:mbox <mailto:tszilagyi@users.sourceforge.net>;
    ];

doap:maintainer [
    foaf:name "MOD Team";
    foaf:homepage <http://portalmod.com>;
    foaf:mbox <mailto:devel@portalmod.com>;
    ];

doap:license <http://usefulinc.com/doap/licenses/gpl>;
lv2:optionalFeature lv2:hardRTCapable;
lv2:minorVersion 7;
lv2:microVersion 2;
lv2:port
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 0;
    lv2:symbol "hornfreq";
    lv2:name "Horn Frequency";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 30;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 1;
    lv2:symbol "bassfreq";
    lv2:name "Rotor Frequency";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 30;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 2;
    lv2:symbol "stwidht";
    lv2:name "Mic Distance";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 100;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 3;
    lv2:symbol "hrbal";
    lv2:name "Rotor/Horn Mix";
    lv2:default 0.5;
    lv2:minimum 0;
    lv2:maximum 1;
],
[
    a lv2:OutputPort, lv2:ControlPort;
    lv2:index 4;
    lv2:symbol "latency";
    lv2:name "latency";
    lv2:default 9200;
    lv2:minimum 0;
    lv2:maximum 9200;
],
[
    a lv2:InputPort, lv2:AudioPort;
    lv2:index 5;
    lv2:symbol "inputl";
    lv2:name "Input L";
],
[
    a lv2:InputPort, lv2:AudioPort;
    lv2:index 6;
    lv2:symbol "inputr";
    lv2:name "Input R";
],
[
    a lv2:OutputPort, lv2:AudioPort;
    lv2:index 7;
    lv2:symbol "outputl";
    lv2:name "Output L";
],
[
    a lv2:OutputPort, lv2:AudioPort;
    lv2:index 8;
    lv2:symbol "outputr";
    lv2:name "Output R";
];
 mod:gui [
        a mod:Gui;
        mod:resourcesDirectory <modgui>;
        mod:iconTemplate <modgui/icon-tap-rotary-speaker.html>;
        mod:templateData <modgui/data-tap-rotary-speaker.json>;
        mod:screenshot <modgui/screenshot-tap-rotary-speaker.png>;
        mod:thumbnail <modgui/thumb-tap-rotary-speaker.png>;
    ] .

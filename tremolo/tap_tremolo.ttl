@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extension/units#> .
@prefix mod: <http://portalmod.com/ns/modgui#>.

<http://portalmod.com/plugins/tap/tremolo>
a lv2:Plugin, lv2:ModulatorPlugin;

doap:name "TAP Tremolo";

doap:developer [
    foaf:name "Tom Szilagyi";
    foaf:homepage <http://tap-plugins.sourceforge.net/>;
    foaf:mbox <mailto:tsziagyi@users.sourceforge.net>;
    ];

doap:maintainer [
    foaf:name "MOD Team";
    foaf:homepage <http://portalmod.com>;
    foaf:mbox <mailto:devel@portalmod.com>;
    ];

doap:license <http://usefulinc.com/doap/licenses/gpl>;
lv2:optionalFeature lv2:hardRTCapable;


lv2:minorVersion 0;
lv2:microVersion 3;
lv2:port
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 0;
    lv2:symbol "frequency";
    lv2:name "Frequency";
    lv2:default 5;
    lv2:minimum 0;
    lv2:maximum 20;
    units:unit units:hz ;
    
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 1;
    lv2:symbol "depth";
    lv2:name "Depth";
    lv2:default 50;
    lv2:minimum 0;
    lv2:maximum 100;
    units:unit units:pc ;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 2;
    lv2:symbol "gain";
    lv2:name "Gain";
    lv2:default 0;
    lv2:minimum -70;
    lv2:maximum 20;
    units:unit units:db ;

],
[
    a lv2:InputPort, lv2:AudioPort;
    lv2:index 3;
    lv2:symbol "input_0";
    lv2:name "Input_0";
],
[
    a lv2:OutputPort,  lv2:AudioPort;
    lv2:index 4;
    lv2:symbol "output_0";
    lv2:name "Output_0";
];
mod:gui [
        a mod:Gui;
        mod:resourcesDirectory <modgui>;
        mod:iconTemplate <modgui/icon-tap-tremolo.html>;
        mod:templateData <modgui/data-tap-tremolo.json>;
        mod:screenshot <modgui/screenshot-tap-tremolo.png>;
        mod:thumbnail <modgui/thumb-tap-tremolo.png>;
    ] .

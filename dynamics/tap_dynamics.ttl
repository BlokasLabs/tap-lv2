@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix mod:   <http://portalmod.com/ns/extensions/effect#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://portalmod.com/plugins/tap/dynamics_m>
a lv2:Plugin, lv2:DynamicsPlugin;

doap:name "TAP Dynamics (M)";

mod:label "TAP Dyn (M)";

doap:developer [
    foaf:name "Tom Szilagyi";
    foaf:homepage <http://tap-plugins.sourceforge.net>;
    foaf:mbox <mailto:tsziagyi@users.sourceforge.net>;
    ];

doap:maintainer [
    foaf:name "MOD Team";
    foaf:homepage <http://portalmod.com>;
    foaf:mbox <mailto:devel@portalmod.com>;
    ];

doap:license <http://usefulinc.com/doap/licenses/gpl>;
lv2:minorVersion 0;
lv2:microVersion 1;
lv2:port
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 0;
    lv2:symbol "AttackMs";
    lv2:name "Attack [ms]";
    lv2:default 128;
    lv2:minimum 4;
    lv2:maximum 500;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 1;
    lv2:symbol "ReleaseMs";
    lv2:name "Release [ms]";
    lv2:default 502;
    lv2:minimum 4;
    lv2:maximum 1000;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 2;
    lv2:symbol "OffsetGainDb";
    lv2:name "Offset Gain [dB]";
    lv2:default 0;
    lv2:minimum -20;
    lv2:maximum 20;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 3;
    lv2:symbol "MakeupGainDb";
    lv2:name "Makeup Gain [dB]";
    lv2:default 0;
    lv2:minimum -20;
    lv2:maximum 20;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 4;
    lv2:symbol "Function";
    lv2:name "Function";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 14.1;
    lv2:portProperty lv2:integer;
    lv2:portProperty lv2:enumeration ;
    lv2:scalePoint
    [
        rdfs:label "2:1 comp at -6 dB";
        rdf:value 0
    ],
    [
        rdfs:label "2:1 comp at -9 dB";
        rdf:value 1
    ],
    [
        rdfs:label "2:1 comp at -12 dB";
        rdf:value 2
    ],
    [
        rdfs:label "2:1 comp at -18 dB";
        rdf:value 3
    ],
    [
        rdfs:label "2.5:1 comp at -12 dB";
        rdf:value 4
    ],
    [
        rdfs:label "3:1 comp at -12 dB";
        rdf:value 5
    ],
    [
        rdfs:label "3:1 comp at -15 dB";
        rdf:value 6
    ],
    [
        rdfs:label "Compressor/Gate";
        rdf:value 7
    ],
    [
        rdfs:label "Expander";
        rdf:value 8
    ],
    [
        rdfs:label "Hard limiter at -6 dB";
        rdf:value 9
    ],
    [
        rdfs:label "Hard limiter at -12 dB";
        rdf:value 10
    ],
    [
        rdfs:label "Hard gate at -35 dB";
        rdf:value 11
    ],
    [
        rdfs:label "Soft limiter";
        rdf:value 12
    ],
    [
        rdfs:label "Soft knee comp/gate";
        rdf:value 13
    ],
    [
        rdfs:label "Soft gate below -36 dB";
        rdf:value 14
    ]
],
[
    a lv2:OutputPort, lv2:ControlPort;
    lv2:index 5;
    lv2:symbol "EnvelopeVolumeDb";
    lv2:name "Envelope Volume [dB]";
    lv2:default 0;
    lv2:minimum -60;
    lv2:maximum 20;
],
[
    a lv2:OutputPort, lv2:ControlPort;
    lv2:index 6;
    lv2:symbol "GainAdjustmentDb";
    lv2:name "Gain Adjustment [dB]";
    lv2:default 0;
    lv2:minimum -60;
    lv2:maximum 20;
],
[
    a lv2:InputPort, lv2:AudioPort;
    lv2:index 7;
    lv2:symbol "Input";
    lv2:name "Input";
],
[
    a lv2:OutputPort, lv2:AudioPort;
    lv2:index 8;
    lv2:symbol "Output";
    lv2:name "Output";
].



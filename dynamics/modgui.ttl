@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .
@prefix ui:     <http://lv2plug.in/ns/extensions/ui#> .

<http://moddevices.com/plugins/tap/dynamics>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-tap-mono-dynamics.html> ;
        modgui:stylesheet <modgui/stylesheet-tap-mono-dynamics.css> ;
        modgui:screenshot <modgui/screenshot-tap-mono-dynamics.png> ;
        modgui:thumbnail <modgui/thumbnail-tap-mono-dynamics.png> ;
        modgui:brand "TAP" ;
        modgui:label "Mono Dynamics" ;
        modgui:model "japanese" ;
        modgui:panel "4-knobs" ;
        modgui:color "red" ;
        modgui:knob "cream" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "attack" ;
            lv2:name "Attack" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "release" ;
            lv2:name "Release" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "offset" ;
            lv2:name "Offs Gain" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "makeup" ;
            lv2:name "Mkup Gain" ;
        ] ;
    ] .
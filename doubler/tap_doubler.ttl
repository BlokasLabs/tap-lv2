@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extension/units#> .
@prefix mod: <http://portalmod.com/ns/modgui#>.

<http://portalmod.com/plugins/tap/doubler>
a lv2:Plugin, lv2:SimulatorPlugin;

doap:name "TAP Fractal Doubler";

mod:label "TAP FracDoubler";

doap:developer [
    foaf:name "Tom Szilagyi";
    foaf:homepage <http://tap-plugins.sourceforge.net/>;
    foaf:mbox <mailto:tsziagyi@users.sourceforge.net>;
    ];

doap:maintainer [
    foaf:name "MOD Team";
    foaf:homepage <http://portalmod.com>;
    foaf:mbox <mailto:devel@portalmod.com>;
    ];

doap:license <http://usefulinc.com/doap/licenses/gpl>;
lv2:minorVersion 0;
lv2:microVersion 1;
lv2:port
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 0;
    lv2:symbol "TimeTracking";
    lv2:name "Time Tracking";
    lv2:default 0.50;
    lv2:minimum 0;
    lv2:maximum 1;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 1;
    lv2:symbol "PitchTracking";
    lv2:name "Pitch Tracking";
    lv2:default 0.50;
    lv2:minimum 0;
    lv2:maximum 1;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 2;
    lv2:symbol "DryLevelDb";
    lv2:name "Dry Level [dB]";
    lv2:default 0;
    lv2:minimum -90;
    lv2:maximum 20;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 3;
    lv2:symbol "DryLeftPosition";
    lv2:name "Dry Left Position";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 1;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 4;
    lv2:symbol "DryRightPosition";
    lv2:name "Dry Right Position";
    lv2:default 1;
    lv2:minimum 0;
    lv2:maximum 1;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 5;
    lv2:symbol "WetLevelDb";
    lv2:name "Wet Level [dB]";
    lv2:default 0;
    lv2:minimum -90;
    lv2:maximum 20;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 6;
    lv2:symbol "WeLeftPosition";
    lv2:name "Wet Left Position";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 1;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 7;
    lv2:symbol "WetRightPosition";
    lv2:name "Wet Right Position";
    lv2:default 1;
    lv2:minimum 0;
    lv2:maximum 1;
],
[
    a lv2:InputPort, lv2:AudioPort;
    lv2:index 8;
    lv2:symbol "Input_L";
    lv2:name "Input_L";
],
[
    a lv2:InputPort, lv2:AudioPort;
    lv2:index 9;
    lv2:symbol "Input_R";
    lv2:name "Input_R";
],
[
    a lv2:OutputPort, lv2:AudioPort;
    lv2:index 10;
    lv2:symbol "Output_L";
    lv2:name "Output_L";
],
[
    a lv2:OutputPort, lv2:AudioPort;
    lv2:index 11;
    lv2:symbol "Output_R";
    lv2:name "Output_R";
];
    mod:gui [
        a mod:Gui;
        mod:resourcesDirectory <modgui>;
        mod:iconTemplate <modgui/icon-tap-fractal-doubler.html>;
        mod:templateData <modgui/data-tap-fractal-doubler.json>;
        mod:screenshot <modgui/screenshot-tap-fractal-doubler.png>;
        mod:thumbnail <modgui/thumb-tap-fractal-doubler.png>;
    ] .

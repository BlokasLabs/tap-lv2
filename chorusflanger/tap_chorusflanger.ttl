@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extension/units#> .
@prefix mod: <http://portalmod.com/ns/modgui#>.


<http://portalmod.com/plugins/tap/chorusflanger>
a lv2:Plugin, lv2:ModulatorPlugin;

doap:name "TAP Chorus/Flanger";

doap:developer [
    foaf:name "Tom Szilagyi";
    foaf:homepage <http://tap-plugins.sourceforge.net/>;
    foaf:mbox <mailto:tsziagyi@users.sourceforge.net>;
    ];

doap:maintainer [
    foaf:name "MOD Team";
    foaf:homepage <http://portalmod.com>;
    foaf:mbox <mailto:devel@portalmod.com>;
    ];

doap:license <http://usefulinc.com/doap/licenses/gpl>;
lv2:optionalFeature lv2:hardRTCapable;

mod:pedalModel "japanese";
mod:pedalColor "purple";
mod:brand "TAP";
mod:pedalLabel "Chorus/Flanger";
mod:smallLabel "Chorus/Flang.";
mod:bypassLedPosition "118-83";

lv2:minorVersion 7;
lv2:microVersion 2;
lv2:port
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 0;
    lv2:symbol "Frequency";
    lv2:name "Frequency";
    lv2:default 1.75;
    lv2:minimum 0;
    lv2:maximum 5;
    units:unit units:hz ;
    mod:iconLabel "Frequency";
    mod:pedalButton "0-0";
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 1;
    lv2:symbol "LRPhaseShift";
    lv2:name "L/R Phase Shift";
    lv2:default 90;
    lv2:minimum 0;
    lv2:maximum 180;
    units:unit units:degree ;
    mod:iconLabel "LR Phase Shft";
    mod:pedalButton "0-2";
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 2;
    lv2:symbol "Depth";
    lv2:name "Depth";
    lv2:default 75;
    lv2:minimum 0;
    lv2:maximum 100;
    units:unit units:pc ;
    mod:iconLabel "Depth";
    mod:pedalButton "0-4";
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 3;
    lv2:symbol "Delay";
    lv2:name "Delay";
    lv2:default 25;
    lv2:minimum 0;
    lv2:maximum 100;
    units:unit units:ms ;
    mod:iconLabel "Delay";
    mod:pedalButton "0-6";
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 4;
    lv2:symbol "Contour";
    lv2:name "Contour";
    lv2:default 100;
    lv2:minimum 20;
    lv2:maximum 20000;
    lv2:portProperty epp:logarithmic ;
    units:unit units:hz ;
    mod:iconLabel "Contour";
    mod:pedalButton "1-3";
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 5;
    lv2:symbol "DryLevel";
    lv2:name "Dry Level";
    lv2:default -3;
    lv2:minimum -90;
    lv2:maximum 20;
    units:unit units:db ;
    mod:iconLabel "Dry";
    mod:pedalButton "2-4";
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 6;
    lv2:symbol "WetLevel";
    lv2:name "Wet Level";
    lv2:default -3;
    lv2:minimum -90;
    lv2:maximum 20;
    units:unit units:db ;
    mod:iconLabel "Wet";
    mod:pedalButton "2-2";
],
[
    a lv2:InputPort, lv2:AudioPort;
    lv2:index 7;
    lv2:symbol "Input_L";
    lv2:name "Input_L";
],
[
    a lv2:InputPort, lv2:AudioPort;
    lv2:index 8;
    lv2:symbol "Input_R";
    lv2:name "Input_R";
],
[
    a lv2:OutputPort, lv2:AudioPort;
    lv2:index 9;
    lv2:symbol "Output_L";
    lv2:name "Output_L";
],
[
    a lv2:OutputPort, lv2:AudioPort;
    lv2:index 10;
    lv2:symbol "Output_R";
    lv2:name "Output_R";
];
mod:gui [
        a mod:Gui;
        mod:resourcesDirectory <modgui>;
        mod:iconTemplate <modgui/icon-tap-chorusflanger.html>;
        mod:templateData <modgui/data-tap-chorusflanger.json>;
        mod:screenshot <modgui/screenshot-tap-chorusflanger.png>;
        mod:thumbnail <modgui/thumb-tap-chorusflanger.png>;
    ] .
